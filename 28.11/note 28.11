#include <iostream>
#include <math.h>
#include <iomanip>

using namespace std;

int main(){

	float a, b, c, d, e;
	float as;
	float resultsd;

	cin >> a;
	cin >> b;
	cin >> c;
	cin >> d;
	cin >> e;
	as = (a + b + c + d + e) / 5;
	cout << setprecision (2)<< fixed << as << endl;

	resultsd = sqrt((pow(a - as, 2) + pow(b - as, 2) + pow(c - as, 2) + pow(d - as, 2) + pow(e - as, 2)) / 5);

	cout << setprecision (2) << fixed<<  resultsd << endl;

	
}

